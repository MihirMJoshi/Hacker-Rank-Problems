#include<bits/stdc++.h>
using namespace std;
int main()
{
    string s;
    cin>> s;
    auto n = s.size();
    int row = round(sqrt(n));
    int column;
    if (row>=sqrt(n))
        column=row;
    else
        column=row+1;
    for (int i=0; i<column;i++)
        {
        for(int j=i; j<n; j+=column)
            cout<<s[j];
            cout<<" ";
        }
    return 0;
}
