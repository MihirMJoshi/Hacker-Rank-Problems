#include <bits/stdc++.h>

using namespace std;

int equalizeArray(vector <int> arr, int freq[101]) 
{
    int z=0;
    for (int i=0; i<101; i++)
        z=max(z,freq[i]);
    return (arr.size()-z);
}

int main() {
    int n;
    cin >> n;
    vector<int> arr(n);
    int freq[101]={};
    for(int arr_i = 0; arr_i < n; arr_i++)
    {
       cin >> arr[arr_i];
       freq[arr[arr_i]]++;
    }
    int result = equalizeArray(arr,freq);
    cout << result << endl;
    return 0;
}
