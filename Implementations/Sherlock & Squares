#include <bits/stdc++.h>
#include<cmath>

using namespace std;

void squares(long long int a, long long int b) 
{
    long long int h = sqrt(b); long long int l = sqrt(a);
    if (h!=l)
    {
        if (l*l!=a)
            cout<<h-l<<endl;
        else
            cout<<h-l+1<<endl;
    }
    else 
    {
        if (l*l==a)
            cout<<h-l+1<<endl;
        else
            cout<<h-l<<endl;
    }
}

int main() {
    int q;
    cin >> q;
    for(int a0 = 0; a0 < q; a0++)
    {
        long long int a;
        long long int b;
        cin >> a >> b;
        squares(a, b);
        //cout << result << endl;
    }
    return 0;
}
