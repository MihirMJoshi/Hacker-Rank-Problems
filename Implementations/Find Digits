#include <bits/stdc++.h>

using namespace std;

int findDigits(int n) 
{
    int r=n;
    int c=0;
        while (r>0)
        {
            if (((r % 10) != 0) && (n % (r % 10) == 0))
                c++;
            r /= 10;
        }
    return c;
}

int main() 
{
    int t;
    cin >> t;
    for(int a0 = 0; a0 < t; a0++){
        int n;
        cin >> n;
        int result = findDigits(n);
         cout << result << endl;
    }
    return 0;
}
