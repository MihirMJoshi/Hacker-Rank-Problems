#include <bits/stdc++.h>

using namespace std;

vector <int> solve(vector < int > grades)
{
    unsigned int i;
    for (i=0;i<grades.size();i++)
    {
        if (grades.at(i)<38)
            cout<<grades.at(i)<<endl;
        else if (grades.at(i)%5 >=3)
    
            cout<<(grades.at(i)+(5-(grades.at(i)%5)))<<endl;
        else
        	cout<<grades.at(i)<<endl;
    }  
    vector <int> l(0);
    return l;
}    

int main() {
    int n;
    cin >> n;
    //cout <<endl;
    vector<int> grades(n);
    for(int grades_i = 0; grades_i < n; grades_i++)
    {
       cin >> grades[grades_i];
    }
    vector < int > result = solve(grades);
    for (ssize_t i = 0; i < result.size(); i++) 
    {
        cout << result[i] << (i != result.size() - 1 ? "\n" : "");
    }
    cout << endl;
    

    return 0;
}
