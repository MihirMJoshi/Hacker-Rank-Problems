#include <bits/stdc++.h>

using namespace std;

int main() 
{
    int minimum;
    int n;
    int t;
    cin >> n >> t;
    vector<int> width(n);
    for(int i = 0; i < n; i++)
       cin >> width[i];
    
   int lower_end[t], upper_end[t];
    
   for (int i=0; i<t; i++)
       cin >> lower_end[i] >> upper_end[i];
    
   for(int i=0; i<t; i++)
   {
      minimum=width[lower_end[i]];
       for(int j=lower_end[i]; j<=upper_end[i]; j++)
           minimum=min(minimum, width[j]);
       cout<<minimum<<endl;
   }
       

    return 0;
}
