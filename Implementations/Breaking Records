#include <bits/stdc++.h>

using namespace std;

void breakingRecords(vector <int> score) 
{
    int max=score.at(0);
    int min=score.at(0);
    int l=0, h=0;
    for (int i=0; i<score.size(); i++)
    {
        if(score.at(i)>max)
        {    
        max=score.at(i);
        h++;
        }    
     }
    for (int i=0; i<score.size(); i++)
    {
        if (score.at(i)<min)
        {
            min=score.at(i);
            l++;
        }
    }    
    cout<<h<<" "<<l;       
}

int main() 
    {
    int n;
    cin >> n;
    vector<int> score(n);
    for(int score_i = 0; score_i < n; score_i++){
       cin >> score[score_i];
    }
    breakingRecords(score);
    //for (ssize_t i = 0; i < result.size(); i++) {
    //    cout << result[i] << (i != result.size() - 1 ? " " : "");
    //}
    cout << endl;


    return 0;
}
