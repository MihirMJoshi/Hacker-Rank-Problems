#include <bits/stdc++.h>

using namespace std;

void Taum_and_birthday(int b, int w, int x, int y, int z)
{
   if (z >= abs(x-y)) 
        cout<<(b*x + w*y)<<endl;
    else if (x <= y) 
        cout<<((b+w)*x + w*z)<<endl;
    else 
        cout<<((b+w)*y + b*z)<<endl; 
}

int main()
{
    int t,w,b,x,y,z;
    cin>>t;
    for (int i=0; i<t; i++)
    {
        cin>>b>>w>>x>>y>>z;
        Taum_and_birthday (b,w,x,y,z);
    }  
    return 0;
}
