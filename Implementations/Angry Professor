#include <bits/stdc++.h>

using namespace std;

void angryProfessor(int k, vector <int> a) 
{
    int c=0;
    for (int i=0; i<a.size(); i++)
    {
        if (a.at(i) <= 0)
            c++;
    }
    if (c >= k)
        cout<<"NO \n";
    else
        cout<<"YES \n";
    // Complete this function
}

int main() {
    int t;
    cin >> t;
    for(int a0 = 0; a0 < t; a0++)
    {
        int n;
        int k;
        cin >> n >> k;
        vector<int> a(n);
            for(int a_i = 0; a_i < n; a_i++)
            {
                cin >> a[a_i];
            }
        angryProfessor(k, a);
        // cout << result << endl;
    }
    return 0;
}
