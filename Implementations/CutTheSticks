#include <bits/stdc++.h>

using namespace std;

void cutTheSticks(vector <int> arr) 
{    
   sort(arr.begin(),arr.end());
   reverse(arr.begin(),arr.end());
    
    while (!arr.empty())
    {
        cout<<arr.size()<<endl;
        for (int i=0;i<arr.size();i++)
        arr.at(i) -= arr.at(arr.size()-1);
        while (!arr.empty() && arr.back()==0)
            arr.pop_back();
    }
}

int main() 
{
    int n;
    cin >> n;
    vector<int> arr(n);
    for(int arr_i = 0; arr_i < n; arr_i++)
    {
       cin >> arr[arr_i];
    }
    cutTheSticks(arr);   
    return 0;
}
