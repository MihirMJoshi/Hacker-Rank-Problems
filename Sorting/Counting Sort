#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n;                                                  // number of elements in an array
    cin>>n;
    
    int freq[101]={0};                                      // variables taken
    vector<int> elements(n);
    for (int i=0; i<n; i++)                                 // inputs taken
    {
        cin>>elements[i];
        freq[elements[i]]++;                                // frequency of elements taken
    }
    
    for (int i=0; i<100; i++)
        cout<<freq[i]<<" ";                                 // dislaying the output
    return 0;
}
